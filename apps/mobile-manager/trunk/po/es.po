msgid "Change PIN code"
msgstr "Cambiar código PIN"

#: ../data/glade/mm_pin_dialog.glade.h:6
msgid "Confirm PIN code : "
msgstr "Confirma el nuevo PIN: "

#: ../data/glade/mm_pin_dialog.glade.h:7
msgid "Enter PIN code"
