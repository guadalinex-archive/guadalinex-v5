Depends: ${shlibs:Depends}, ${misc:Depends}, ${python:Depends}, ubiquity, ubiquity-frontend-gtk, restricted-manager, mythtv-common, python-mysqldb, expect, guidance-backends, vnc4-common, lirc (>= 0.8.2-0ubuntu3), mythbuntu-common
