#. Description
#: ../ubiquity-frontend-mythbuntu.templates:32001
msgid "Myth Gallery"
msgstr "Myth Gallery"

#. Type: text
#. Description
#: ../ubiquity-frontend-mythbuntu.templates:33001
msgid "Myth Game"
msgstr "Myth משחקים"

#. Type: text
#. Description
#: ../ubiquity-frontend-mythbuntu.templates:34001
msgid "Myth Music"
msgstr "Myth מוסיקה"

