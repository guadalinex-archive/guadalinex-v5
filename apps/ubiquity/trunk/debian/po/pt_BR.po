msgid "Myth Flix"
msgstr "Myth Flix"

#. Type: text
#. Description
#: ../ubiquity-frontend-mythbuntu.templates:32001
msgid "Myth Gallery"
msgstr "Galeria Myth"

#. Type: text
#. Description
#: ../ubiquity-frontend-mythbuntu.templates:33001
msgid "Myth Game"
msgstr "Jogo Myth"

#. Type: text
#. Description
#: ../ubiquity-frontend-mythbuntu.templates:34001
msgid "Myth Music"
msgstr "Música Myth"

#. Type: text
#. Description
#: ../ubiquity-frontend-mythbuntu.templates:35001
msgid "Myth News"
msgstr "Notícias Myth"

